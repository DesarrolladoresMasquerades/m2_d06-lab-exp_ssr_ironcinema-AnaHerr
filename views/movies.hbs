<h2>Ironhack Cinema</h2>
<h2>Click on the movie to check the showtimes!</h2>


{{#each movies}}
<div class="card" style="width: 18rem;">
  <img src="{{image}}" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">Title:{{title}}</h5>
    <a href="/movies/{{_id}}" class="btn btn-primary">See more</a>
  </div>
</div>
{{/each}} 
    